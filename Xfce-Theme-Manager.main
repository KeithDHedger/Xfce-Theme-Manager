###MAIN###
init ()
{
	GTKDIALOG=gtkdialog
	DATA=25
	RESET=/tmp/xfce-theme-manager$$

	export -f liveupdate
	
	export DATA
}

liveupdate ()
{
	echo "Data = $DATA setting to $DATA_RANGE" >&2
}

init
main
rm $RESET
