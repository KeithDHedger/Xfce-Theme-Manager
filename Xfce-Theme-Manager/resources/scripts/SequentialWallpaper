NUMMONITORS=2
BORDERSARRAY=
ARRAYNUM=0

makeArray ()
{
	while read
		do
			BORDERSARRAY[$ARRAYNUM]="$REPLY"
			((ARRAYNUM=ARRAYNUM+1))
		done< <(xfce-theme-manager -l b)
}


makeArray

if [ -e ~/.config/XfceThemeManager/wallpapernumber ];then
:
else
:
fi